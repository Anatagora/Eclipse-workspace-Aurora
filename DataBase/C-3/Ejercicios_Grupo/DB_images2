CREATE DATABASE db_images2;
 USE db_images2; 

CREATE TABLE memes (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(255) NOT NULL,
    image LONGBLOB NOT NULL
);
 
